{
    "name": "another-マウスの軌跡を新しいタブに表示させてくれる拡張機能",
    "description" : "ポップアップからスクリーンショットを撮ると、マウスの軌跡を新しいタブに表示させることができます。",
    "version": "1.1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["js/p5.min.js", "js/content-script.js"],
          "css":["css/style.css"]
        }
    ],
    "action": {
        "default_popup": "html/popup.html"
    },
    "permissions": [
        "tabs", "storage"
      ],
      "chrome_url_overrides":{
        "newtab":"html/my-new-tab.html"
      },
      "icons": {
        "16": "/images/logo.png",
        "32": "/images/logo@2x.png",
        "48": "/images/logo@3x.png",
        "128": "/images/logo@8x.png"
      }
}